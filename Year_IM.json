{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": 450,
  "data": {
    "url": "https://raw.githubusercontent.com/ohol0007/FIT3179/main/data/FIT3179%20Assignment%202%20Data.csv"
  },
  "transform": [
    {"filter": "datum.Infant_Deaths > 0"}
  ],
  "mark":"bar",
  "encoding": {
    "x": {
      "field": "Year"
    },
    "y": {
      "field": "Infant_Deaths",
      "scale": {
        "type": "linear"
      }
    }
  }
}
